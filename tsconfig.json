{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "outDir": "./backend/dist",
    "rootDir": "./backend/src",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "backend/src/**/*"
  ],
  "exclude": [
    "node_modules",
    "backend/dist"
  ]
}
